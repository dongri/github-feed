{
  "manifest_version": 2,
  "name": "GitHub Feed",
  "version": "1.0",
  "description": "The GitHub feed",
  "permissions": [
    "https://github.com/"
  ],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
